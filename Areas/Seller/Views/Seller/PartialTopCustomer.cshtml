@model IEnumerable<SellerVendor.Areas.Seller.Models.TopProduct>  
@{
    ViewBag.Title = "PartialTopCustomer";
}
<table class="table table-hover">
    <thead>
        <tr>
            <th>Name</th>
            @*<th>Price</th>
            <th>Quantity</th>*@
        </tr>
    </thead>
    <tbody>
        @foreach (var customer in Model)  
        {  
  
            <tr>  
                <td>@customer.Name</td>  
              @*  <td>Rs. @product.Price</td>  
                <td class="text-center">@product.Quantity</td>  *@
            </tr>  
        }  
    </tbody>
</table>