@model IEnumerable<SellerVendor.Areas.Seller.Models.TopProduct>  
@{
    ViewBag.Title = "PartialTopSeller";
}
<table class="table table-hover">
    <thead>
        <tr>
            <th>Product</th>
            <th>SKU No</th>
            <th>Price</th>
            <th>Quantity</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var product in Model)  
        {    
            <tr>  
                <td>@product.Name</td>  
                <td>@product.SKU</td>
                <td>Rs.@product.Price</td>  
                <td class="text-center">@product.Quantity</td>   
            </tr>  
        }  
    </tbody>
</table>
