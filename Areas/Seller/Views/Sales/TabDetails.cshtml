@model  SellerVendor.Areas.Seller.Models.Tab
@{
    ViewBag.Title = "TabDetails";
    Layout = "~/Areas/Seller/Views/Shared/_Layout.cshtml";
}
<link href="~/Areas/Seller/TabCss/jquery-ui.css" rel="stylesheet" />
<script src="~/Areas/Seller/TabCss/jquery-ui.js"></script>
<script src="~/Areas/Seller/TabCss/jquery-1.10.2.js"></script>
<link href="~/Areas/Seller/SellerScripts/angucomplete-alt.css" rel="stylesheet" />
 

<script>
   function SaleInvoicePrint(item) {         
      var str = item.split("_");
      var id = str[1];
      $.ajax({
       url: '/Seller/Sales/SaleInvoicePrintForDetails',
       datatype: "json",
       type: "post",
       contenttype: 'application/json; charset=utf-8',
       async: true,
       data: { id: id },            
       success: function (data) {
         $("#empList").html(data);                 
         $('#myModal3').modal({
            show: 'false'
           });
         },
       error: function (xhr) {
          alert('error');
         }
     });
 }
</script>
<style>
        body
        {
            font-size: 11px;
        }
    </style>
<script>
    $(function () {
     $("#countrytabs").tabs();
    });
</script>
<style>    
    .sub-total {
        width: 100%;
        height: auto;
        float: left;
    }

        .sub-total table tbody {
            width: 100%;
            height: auto;
        }

            .sub-total table tbody tr {
                width: 100%;
                float: left;
                height: auto;
                margin: 0px 0px 5px;
            }

    .total-left {
        width: 59%;
        float: left;
        padding: 15px 4px 0px;
        text-align: right;
    }

    .total-right {
        width: 41%;
        float: right;
        height: auto;
        padding: 7px;
    }

        .total-right input {
            margin: 0px;
            width: 100%;
            height: 26px;
        }

    .discount-left {
        width: 48px !important;
        float: left;
    }

    .discount-right {
        width: 105px !important;
        float: right;
    }
</style>

<div class="row" ng-app="MyAppseller2" ng-controller="MySalesController">
