<script>
    function PostGuidNo() {
        var ret = true;
        var guid = $("#txt_guid").val();
        var accounting_sft_id = $("#ddl_tally").val();
        if (guid == '') {
            $("#txt_guid").css({ 'border': '1px solid red' });
            $("#txt_guid").focus();
            ret = false;
        } else
            $("#txt_guid").removeAttr('style');
        if (ret)
            Post(guid, accounting_sft_id);
        else
            return ret;
        function Post(guid, accounting_sft_id) {
            $.ajax({
                type: "POST",
                url: "/SellerSetting/SaveGuidNumber",
                dataType: "json",
                data: {
                    txt_guid: guid, ddl_tally: accounting_sft_id
                },
                success: function (result) {
                    $('#msgg').text(result);
                    Guidclearform();
                },
                error: function (result) {
                    alert('error');
                }
            });
        }
    }
    function Guidclearform() {
        $("#txt_guid").val('');
        //$("#ddl_tally").val('');
    }

    function Postseminar() {
        //alert("abcd");
        var sellerid = $("#sellerid").val();
        var oldpassword = $("#t_oldPassword").val();
        var newpassword = $("#t_NewPassword").val();
        var confirmpassword = $("#t_ConfirmPassword").val();
        $.ajax({
            type: "POST",
            url: "/SellerSetting/SaveChangePassword",
            dataType: "json",
            data: {
                t_oldPassword: oldpassword, t_NewPassword: newpassword, t_ConfirmPassword: confirmpassword,
                tbl_seller_id: sellerid
            },
            success: function (result) {
                alert(result);
                $('#msgg').text(result);
                clearform();
            },
            error: function (result) {
                alert('error');
            }
        });
    }
    function clearform() {       
        $("#t_oldPassword").val() = "";
        $("#t_NewPassword").val() = "";
        $("#t_ConfirmPassword").val() = "";
    }
</script>

<script>        
   function phoneno(){          
      $('#phone').keypress(function(e) {
         var a = [];
         var k = e.which;
         for (i = 48; i < 58; i++)
               a.push(i);
           if (!(a.indexOf(k)>=0))
                e.preventDefault();
            });
         }
    function picocde() {
       $('#pin').keypress(function (e) {
          var a = [];
          var k = e.which;
          for (i = 48; i < 58; i++)
                a.push(i);
           if (!(a.indexOf(k) >= 0))
                e.preventDefault();
           });
        }
</script>

