<script>
    function yesnoCheck() {
        debugger;
        var finaceyear = $('#t_financial_year').val();
        //alert(finaceyear);
        if (document.getElementById('yesCheck').checked) {
            $('#financialyear').val(finaceyear);
            $("#n_financial_status").val(1);
        }
        else {
            $('#financialyear').val('');
            $("#n_financial_status").val(2);
        }
    }


    function GetState(input) {
        //alert("text")
        if ($(input).val() != " ") {
            $.getJSON("/SellerSetting/GetStateList/", { n_CountryId: $(input).val() }, function (data) {
                $('#n_StateId').empty();
                $.each(data, function (i, item) {
                    debugger;
                    $('#n_StateId').append($('<option/>', {
                        value: item.Value,
                        text: item.Text

                    }));
                });
                $('#n_StateId option:first').before('<option value=" ">--Select State--</option>');
                $('#n_StateId').val($('#n_StateId' + "option:contains('Select')").val());
                $('#n_StateId')[0].attr('selected', 'selected');
                //  $('#n_StateId option:selected').val(" ");
            })
        }
        else {
            $('#n_StateId').html('<option value=" ">--Select--</option>');
        }
    }
</script>

